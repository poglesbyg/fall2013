#include <stdio.h>

int main() {
    int age = 23;
    int height = 193;
    // this is a comment
    // and does not do anything

    printf("I am %d years old.\n", age);
    printf("I am %d cm tall.\n", height);
    
    int distance = height * age;
    printf("This is how far you're going to make it: %d.\n", distance);

    return 0;
}
